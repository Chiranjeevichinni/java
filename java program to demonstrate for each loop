import java.io.*;
public class Main
{
	public static void main(String[] args) 
	{
	    int arr[] = {10,20,30,40,50};
	    String mock[] = {"SASI","CHIRANJEEVI","CIC"};
	    System.out.println("The elements in the are: ");
	    for(int I:arr)
	    {
	        System.out.println(I+" ");
	    }
	    System.out.println("Student details are");
	    for(String I:mock)
	    {
	        System.out.println(I+ " ");
	    }
	}
}
